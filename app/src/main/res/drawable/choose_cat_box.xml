<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_checked="false" android:color="@color/colorText">
        <shape android:shape="rectangle">
            <corners
                android:bottomLeftRadius="9dp"
                android:bottomRightRadius="9dp"
                android:topLeftRadius="9dp"
                android:topRightRadius="9dp"></corners>
            <stroke android:width="1dp"
                android:color="#c0c0c0"></stroke>
            <solid android:color="#fafafa"/>
        </shape>
    </item>
</selector>